utton v-hasPermi=\"['permission:user:post']\" type=\"primary\" plain icon=\"el-icon-plus\"\n                            size=\"mini\" @click=\"handleAdd\">新增\n                        </el-button>\n                    </el-col>\n                    <right-toolbar :show-search.sync=\"showSearch\" :columns=\"columns\" @queryTable=\"getList\" />\n                </el-row>\n\n                <el-table v-loading=\"loading\" :data=\"vipCardList\" @selection-change=\"handleSelectionChange\">\n                    <el-table-column type=\"selection\" width=\"50\" align=\"center\" />\n                    <!-- <el-table-column v-if=\"columns[0].visible\" key=\"id\" label=\"用户编号\" align=\"center\" prop=\"id\" /> -->\n                    <el-table-column label=\"序号\" type=\"index\" width=\"50\" align=\"center\">\n                    </el-table-column>\n                    <el-table-column v-if=\"columns[1].visible\" key=\"name\" label=\"用户名称\" align=\"center\" prop=\"name\"\n                        :show-overflow-tooltip=\"true\" />\n                    <el-table-column v-if=\"columns[2].visible\" key=\"originAmount\" label=\"原价(U)\" align=\"center\"\n                        prop=\"originAmount\" :show-overflow-tooltip=\"true\" />\n                    <el-table-column v-if=\"columns[3].visible\" key=\"discountAmount\" label=\"优惠价格(U)\" align=\"center\"\n                        prop=\"discountAmount\" :show-overflow-tooltip=\"true\" />\n                    <el-table-column v-if=\"columns[4].visible\" key=\"status\" label=\"状态\" align=\"center\">\n                        <template slot-scope=\"scope\">\n                            <span v-if=\"scope.row.status === 1\">启用</span>\n                            <span v-else>禁用</span>\n                        </template>\n                    </el-table-column>\n                    <el-table-column v-if=\"columns[5].visible\" label=\"更新时间\" align=\"center\" prop=\"update_datetime\"\n                        width=\"160\">\n                        <template slot-scope=\"scope\">\n                            <span>{{ parseTime(scope.row.update_datetime) }}</span>\n                        </template>\n                    </el-table-column>\n                    <el-table-column v-if=\"columns[5].visible\" label=\"创建时间\" align=\"center\" prop=\"create_datetime\"\n                        width=\"160\">\n                        <template slot-scope=\"scope\">\n                            <span>{{ parseTime(scope.row.create_datetime) }}</span>\n                        </template>\n                    </el-table-column>\n                    <el-table-column v-if=\"hasPermi(['member:vip:post', 'member:vip:post'])\" label=\"操作\"\n                        align=\"center\" width=\"160\" class-name=\"small-padding fixed-width\">\n                        <template slot-scope=\"scope\">\n                            <el-button v-hasPermi=\"['member:vip:post']\" size=\"mini\" type=\"text\" icon=\"el-icon-edit\"\n                                @click=\"handleUpdate(scope.row)\">修改\n                            </el-button>\n                            <el-button v-hasPermi=\"['member:vip:post']\" size=\"mini\" type=\"text\" icon=\"el-icon-edit\"\n                                @click=\"handleDelete(scope.row)\">删除\n                            </el-button>\n                        </template>\n                    </el-table-column>\n                </el-table>\n\n                <pagination v-show=\"total > 0\" :total=\"total\" :page.sync=\"queryParams.pageNum\"\n                    :limit.sync=\"queryParams.pageSize\" @pagination=\"getList\" />\n            </el-col>\n        </el-row>\n\n        <!-- 添加或修改参数配置对话框 -->\n        <el-dialog :title=\"title\" :visible.sync=\"open\" width=\"600px\" append-to-body>\n            <el-form ref=\"form\" :model=\"form\" :rules=\"rules\" label-width=\"100px\">\n                <el-row>\n                    <el-col :span=\"20\">\n                        <el-form-item label=\"会员卡名称\" prop=\"name\">\n                            <el-input v-model=\"form.name\" placeholder=\"请输入会员卡名称\" />\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"12\">\n                        <el-form-item label=\"原价\" prop=\"originAmount\">\n                            <el-input type=\"number\" v-model=\"form.originAmount\" placeholder=\"原价\" maxlength=\"11\" />\n                        </el-form-item>\n                    </el-col>\n                    <el-col :span=\"12\">\n                        <el-form-item label=\"优惠价格\" prop=\"discountAmount\">\n                            <el-input type=\"number\" v-model=\"form.discountAmount\" placeholder=\"优惠价格\" maxlength=\"11\" />\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"12\">\n                        <el-form-item label=\"状态\">\n                            <el-radio-group v-model=\"form.status\">\n                                <el-radio v-for=\"dict in statusOptions\" :key=\"dict.dictValue\" :label=\"dict.dictValue\">{{\n                                    dict.dictLabel }}\n                                </el-radio>\n                            </el-radio-group>\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n                <el-row>\n                    <el-col :span=\"24\">\n                        <el-form-item label=\"备注\">\n                            <el-input v-model=\"form.remark\" type=\"textarea\" placeholder=\"请输入内容\" />\n                        </el-form-item>\n                    </el-col>\n                </el-row>\n            </el-form>\n            <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button type=\"primary\" @click=\"submitForm\">确 定</el-button>\n                <el-button @click=\"cancel\">取 消</el-button>\n            </div>\n        </el-dialog>\n    </div>\n</template>\n  \n<script>\nimport {\n    addVipCard,\n    delVipCard,\n    getVipCard,\n    listVipCard,\n    updateVipCard\n} from \"@/api/admin/member/vipCard\";\n\nexport default {\n    name: \"vipCard\",\n    data() {\n        return {\n            // 遮罩层\n            loading: true,\n            // 选中数组\n            ids: [],\n            // 非单个禁用\n            single: true,\n            // 非多个禁用\n            multiple: true,\n            // 显示搜索条件\n            showSearch: true,\n            // 总条数\n            total: 0,\n            // 用户表格数据\n            vipCardList: null,\n            // 弹出层标题\n            title: \"\",\n            // 是否显示弹出层\n            open: false,\n            // 日期范围\n            dateRange: [],\n            // 状态数据字典\n            statusOptions: [{ dictLabel: \"正常\", dictValue: 1 }, { dictLabel: \"禁用\", dictValue: 0 }],\n            // 表单参数\n            form: {\n                status: 0\n            },\n            defaultProps: {\n                children: \"children\",\n                label: \"label\"\n            },\n\n            // 查询参数\n            queryParams: {\n                pageNum: 1,\n                pageSize: 10,\n                name: undefined,\n                status: 1,\n            },\n            // 列信息\n            columns: [\n                { key: 0, label: `序号`, visible: false },\n                { key: 1, label: `会员卡名称`, visible: true },\n                { key: 2, label: `原价(U)`, visible: true },\n                { key: 3, label: `优惠价(U)`, visible: true },\n                { key: 4, label: `状态`, visible: true },\n                { key: 5, label: `更新时间`, visible: true },\n                { key: 6, label: `创建时间`, visible: true }\n            ],\n            // 表单校验\n            rules: {\n                name: [\n                    { required: true, message: \"名称不能为空\", trigger: \"blur\" }\n                ],\n            }\n        };\n    },\n    watch: {\n\n    },\n    created() {\n        this.getList();\n    },\n    methods: {\n        /** 查询用户列表 */\n        getList() {\n            this.loading = true;\n            listVipCard(this.addDateRange(this.queryParams, this.dateRange)).then(response => {\n                this.vipCardList = response.data.results;\n                this.total = response.data.count;\n                this.loading = false;\n            }\n            );\n        },\n\n        // 筛选节点\n        filterNode(value, data) {\n            if (!value) return true;\n            return data.label.indexOf(value) !== -1;\n        },\n\n        // 用户状态修改\n        handleStatusChange(row) {\n            const text = row.is_active === true ? \"启用\" : \"停用\";\n            this.$confirm('确认要\"' + text + '\"\"' + row.username + '\"用户吗?', \"警告\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\"\n            }).then(function () {\n                return changeUserStatus(row.id, row.is_active);\n            }).then(() => {\n                this.msgSuccess(text + \"成功\");\n            }).catch(function () {\n                row.is_active = row.is_active === false;\n            });\n        },\n        // 取消按钮\n        cancel() {\n            this.open = false;\n            this.reset();\n        },\n        // 表单重置\n        reset() {\n            this.form = {\n                id: undefined,\n                name: undefined,\n                status: 0,\n                remark: undefined,\n                originAmount: undefined,\n                discountAmount: undefined,\n                vipCardType: undefined\n            };\n            this.resetForm(\"form\");\n        },\n        /** 搜索按钮操作 */\n        handleQuery() {\n            this.queryParams.page = 1;\n            this.getList();\n        },\n        /** 重置按钮操作 */\n        resetQuery() {\n            this.dateRange = [];\n            this.resetForm(\"queryForm\");\n            this.handleQuery();\n        },\n        // 多选框选中数据\n        handleSelectionChange(selection) {\n            this.ids = selection.map(item => item.id);\n            this.single = selection.length !== 1;\n            this.multiple = !selection.length;\n        },\n        /** 新增按钮操作 */\n        handleAdd() {\n            this.reset();\n            this.open = true;\n            this.title = \"添加vip卡\";\n        },\n        /** 修改按钮操作 */\n        handleUpdate(row) {\n            // this.reset();\n            const id = row.id || this.ids;\n            getVipCard(id).then(response => {\n                const data = response.data;\n                this.form = data;\n                this.open = true;\n                this.title = \"修改vip卡\";\n            });\n        },\n\n        /** 提交按钮 */\n        submitForm: function () {\n            this.$refs[\"form\"].validate(valid => {\n                if (valid) {\n                    if (this.form.id !== undefined) {\n                        updateVipCard(this.form).then(response => {\n                            this.msgSuccess(\"修改成功\");\n                            this.open = false;\n                            this.getList();\n                        });\n                    } else {\n                        switch (true) {\n                            case this.form.name === '月卡':\n                                this.form.vipCardType = 0;\n                                break;\n                            case this.form.name === '季卡':\n                                this.form.vipCardType = 1;\n                                break;\n                            case this.form.name === '半年卡':\n                                this.form.vipCardType = 2;\n                                break;\n                            case this.form.name === '年卡':\n                                this.form.vipCardType = 3;\n                                break;\n                        }\n                        console.log(this.form)\n                        addVipCard(this.form).then(response => {\n                            this.msgSuccess(\"新增成功\");\n                            this.open = false;\n                            this.getList();\n                        });\n                    }\n                }\n            });\n        },\n        /** 删除按钮操作 */\n        handleDelete(row) {\n            const vipCardId = row.id || this.ids;\n            this.$confirm('是否确认删除该数据', \"警告\", {\n                confirmButtonText: \"确定\",\n                cancelButtonText: \"取消\",\n                type: \"warning\"\n            }).then(function () {\n                return delVipCard(vipCardId);\n            }).then(() => {\n                this.getList();\n                this.msgSuccess(\"删除成功\");\n            });\n        },\n    }\n};\n</script>\n<style lang=\"scss\">\n.el-col-20 {\n    width: 100% !important;\n}\n</style>\n  "],"mappings":";;;;;;;;AAqIA,IAAAA,QAAA,GAAAC,OAAA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;iCAQA;EACAC,IAAA;EACAC,IAAA,WAAAA,KAAA;IACA;MACA;MACAC,OAAA;MACA;MACAC,GAAA;MACA;MACAC,MAAA;MACA;MACAC,QAAA;MACA;MACAC,UAAA;MACA;MACAC,KAAA;MACA;MACAC,WAAA;MACA;MACAC,KAAA;MACA;MACAC,IAAA;MACA;MACAC,SAAA;MACA;MACAC,aAAA;QAAAC,SAAA;QAAAC,SAAA;MAAA;QAAAD,SAAA;QAAAC,SAAA;MAAA;MACA;MACAC,IAAA;QACAC,MAAA;MACA;MACAC,YAAA;QACAC,QAAA;QACAC,KAAA;MACA;MAEA;MACAC,WAAA;QACAC,OAAA;QACAC,QAAA;QACAtB,IAAA,EAAAuB,SAAA;QACAP,MAAA;MACA;MACA;MACAQ,OAAA,GACA;QAAAC,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,GACA;QAAAD,GAAA;QAAAN,KAAA;QAAAO,OAAA;MAAA,EACA;MACA;MACAC,KAAA;QACA3B,IAAA,GACA;UAAA4B,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAC,KAAA,GAEA;EACAC,OAAA,WAAAA,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,O